[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Molokai
Font=monoOne,10,-1,5,50,0,0,0,0,0

[General]
Directory=~/code
Environment=TERM=xterm-256color,JALCINE_HOST=jalcine@stark.jalcine
LocalTabTitleFormat=%w - %u@%h:%D
Name=Regular
Parent=FALLBACK/
RemoteTabTitleFormat=%w - (%u) %H

[Keyboard]
KeyBindings=linux

[Scrolling]
HistoryMode=1
ScrollBarPosition=2

[Terminal Features]
BidiRenderingEnabled=false
BlinkingCursorEnabled=true
FlowControlEnabled=false
